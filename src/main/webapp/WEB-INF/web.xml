<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Contact</display-name>
  <servlet>
  	<servlet-name>Query</servlet-name>
  	<display-name>Query</display-name>
  	<description></description>
  	<servlet-class>contact.Query</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Submit</servlet-name>
  	<display-name>Submit</display-name>
  	<description></description>
  	<servlet-class>contact.Submit</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Insert</servlet-name>
  	<display-name>Insert</display-name>
  	<description></description>
  	<servlet-class>contact.Insert</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Query</servlet-name>
  	<url-pattern>/Query</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Insert</servlet-name>
  	<url-pattern>/Insert/*</url-pattern>
  </servlet-mapping>
</web-app>
